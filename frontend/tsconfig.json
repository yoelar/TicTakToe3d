{
  "compilerOptions": {
    "target": "esnext",
    "lib": [ "DOM", "ES2021" ],
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "types": [ "vite/client" ]
  },
  "include": [ "src" ]
}
